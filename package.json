{
    "name": "web-mojo",
    "version": "2.0.2502",
    "description": "MOJO - A lightweight JavaScript framework for building data-driven web applications",
    "type": "module",
    "main": "./dist/web-mojo.cjs.js",
    "module": "./dist/web-mojo.esm.js",
    "browser": "./dist/web-mojo.umd.js",
    "exports": {
        ".": {
            "import": "./dist/web-mojo.esm.js",
            "require": "./dist/web-mojo.cjs.js",
            "browser": "./dist/web-mojo.umd.js",
            "types": "./dist/web-mojo.d.ts"
        },
        "./auth": {
            "import": "./src/auth.js",
            "require": "./dist/auth.cjs.js",
            "default": "./src/auth.js"
        },
        "./models": {
            "import": "./src/models.js",
            "require": "./dist/models.cjs.js",
            "default": "./src/models.js"
        },
        "./models/*": {
            "import": "./src/models/*.js",
            "default": "./src/models/*.js"
        },
        "./admin": {
            "import": "./src/admin.js",
            "require": "./dist/admin.cjs.js",
            "default": "./src/admin.js"
        },
        "./admin/*": {
            "import": "./src/admin/*.js",
            "default": "./src/admin/*.js"
        },
        "./lightbox": {
            "import": "./src/lightbox.js",
            "require": "./dist/lightbox.cjs.js",
            "default": "./src/lightbox.js"
        },
        "./css/core": "./dist/core.css",
        "./css/portal": "./dist/portal.css",
        "./css/table": "./dist/table.css",
        "./css/auth": "./dist/auth.css",
        "./css/lightbox": "./dist/lightbox.css",
        "./css/*": "./dist/*",
        "./styles/core": "./dist/core.css",
        "./styles/portal": "./dist/portal.css",
        "./styles/table": "./dist/table.css",
        "./styles/auth": "./dist/auth.css",
        "./styles/*": "./dist/*",
        "./components": {
            "import": "./dist/components/index.esm.js",
            "require": "./dist/components/index.cjs.js",
            "default": "./src/components/index.js"
        },
        "./components/*": {
            "import": "./src/components/*.js",
            "default": "./src/components/*.js"
        },
        "./utils": {
            "import": "./dist/utils/index.esm.js",
            "require": "./dist/utils/index.cjs.js",
            "default": "./src/utils/index.js"
        },
        "./utils/*": {
            "import": "./src/utils/*.js",
            "default": "./src/utils/*.js"
        },
        "./templates": "./dist/templates/index.js",
        "./templates/*": "./dist/templates/*",
        "./src/*": "./src/*",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "src",
        "README.md",
        "LICENSE",
        "CHANGELOG.md",
        "css-manifest.json"
    ],
    "scripts": {
        "dev": "concurrently \"npm run build:templates:watch\" \"vite\"",
        "dev:live": "node serve-dev.js",
        "dev:watch": "vite build --watch",
        "dev:full": "concurrently \"npm run build:templates:watch\" \"npm run version:watch\" \"npm run dev\" \"npm run test:watch\"",
        "dev:framework": "concurrently \"npm run build:templates:watch\" \"npm run build:lib:watch\" \"npm run dev\"",
        "dev:version": "concurrently \"npm run version:watch\" \"npm run dev\"",
        "dev:link": "npm link && npm run dev:framework",
        "dev:templates": "npm run build:templates:watch",
        "serve": "node serve-examples.js",
        "examples": "node serve-examples.js",
        "build": "vite build",
        "build:templates": "node scripts/build-templates.js",
        "build:templates:watch": "node scripts/build-templates.js --watch",
        "build:lib": "npm run build:templates && vite build --config vite.config.lib.js",
        "build:lib:watch": "vite build --config vite.config.lib.js --watch",
        "build:css": "cp src/css/core.css dist/core.css && cp src/css/portal.css dist/portal.css && cp src/css/table.css dist/table.css && cp src/css/auth.css dist/auth.css",
        "build:dist": "npm run clean && npm run build:templates && npm run build:lib && npm run build:css",
        "build:watch": "vite build --watch",
        "clean": "rm -rf dist",
        "preview": "vite preview",
        "prepublishOnly": "npm run build:dist && npm run test",
        "postpublish": "echo 'Successfully published web-mojo@'$npm_package_version",
        "debug": "node test/utils/debug-server.js",
        "debug:watch": "node test/utils/debug-server.js --watch",
        "scss": "sass src/styles/main.scss dist/styles/mojo.css --watch",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "migrate:camelcase": "node scripts/migrate-to-camelcase.js",
        "migrate:camelcase:dry": "node scripts/migrate-to-camelcase.js --dry-run",
        "migrate:camelcase:backup": "node scripts/migrate-to-camelcase.js --backup",
        "test": "node test/test-runner.js",
        "test:unit": "node test/test-runner.js --suite unit",
        "test:integration": "node test/test-runner.js --suite integration",
        "test:build": "node test/test-runner.js --suite build",
        "test:watch": "node test/test-runner.js --watch",
        "test:coverage": "node test/test-runner.js --coverage",
        "test:migrate": "node test/test-runner.js --migrate",
        "version:watch": "node scripts/version-manager.js --watch",
        "version:increment": "node scripts/version-manager.js --increment",
        "version:init": "node scripts/version-manager.js --init"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/web-mojo.git"
    },
    "keywords": [
        "mojo",
        "web-mojo",
        "framework",
        "javascript",
        "frontend",
        "mvc",
        "spa",
        "single-page-application",
        "bootstrap",
        "bootstrap5",
        "es6",
        "esm",
        "vite",
        "rest",
        "restful",
        "data-driven",
        "component-based"
    ],
    "author": "MOJO Framework Team",
    "license": "Apache-2.0",
    "homepage": "https://github.com/yourusername/web-mojo#readme",
    "bugs": {
        "url": "https://github.com/yourusername/web-mojo/issues"
    },
    "peerDependencies": {
        "bootstrap": "^5.3.0"
    },
    "peerDependenciesMeta": {
        "bootstrap": {
            "optional": false
        }
    },
    "dependencies": {
        "@toast-ui/editor": "^3.2.2",
        "mustache": "^4.2.0"
    },
    "devDependencies": {
        "bootstrap": "^5.3.2",
        "chokidar": "^3.6.0",
        "concurrently": "^9.2.0",
        "eslint": "^8.54.0",
        "express": "^4.21.2",
        "jsdom": "^26.1.0",
        "node-fetch": "^3.3.2",
        "sass": "^1.69.0",
        "terser": "^5.24.0",
        "vite": "^7.1.0",
        "vm2": "^3.9.19",
        "ws": "^8.18.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "sideEffects": false
}
