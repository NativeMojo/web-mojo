; Mustache double braces
("{{" @open "}}" @close)

; Mustache triple braces (unescaped)
("{{{" @open "}}}" @close)

; HTML brackets
("<" @open ">" @close)

; Double quotes
("\"" @open "\"" @close)

; Single quotes
("'" @open "'" @close)

; Parentheses
("(" @open ")" @close)

; Square brackets
("[" @open "]" @close)

; Curly braces (for HTML/CSS content)
("{" @open "}" @close)
