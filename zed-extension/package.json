{
  "name": "mojo-mustache-zed-extension",
  "version": "1.0.0",
  "description": "Zed language extension for MOJO Mustache (.mst) templates",
  "main": "extension.json",
  "scripts": {
    "install": "bash install.sh",
    "uninstall": "bash install.sh uninstall",
    "verify": "bash install.sh verify",
    "test": "echo \"Testing extension with test.mst file\"",
    "lint": "echo \"Linting grammar files\"",
    "package": "tar -czf mojo-mustache-zed-extension.tar.gz extension.json languages/ grammars/ README.md",
    "clean": "rm -f *.tar.gz"
  },
  "keywords": [
    "zed",
    "zed-extension",
    "mustache",
    "mojo",
    "template",
    "syntax-highlighting",
    "language-support",
    "mst"
  ],
  "author": "MOJO Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/web-mojo.git",
    "directory": "zed-extension"
  },
  "homepage": "https://github.com/your-org/web-mojo#readme",
  "bugs": {
    "url": "https://github.com/your-org/web-mojo/issues"
  },
  "engines": {
    "zed": ">=0.100.0"
  },
  "files": [
    "extension.json",
    "languages/",
    "grammars/",
    "README.md",
    "install.sh",
    "install.bat"
  ],
  "extensionKind": [
    "language"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mojo-mustache",
        "aliases": [
          "MOJO Mustache",
          "mojo-mustache",
          "mst"
        ],
        "extensions": [
          ".mst"
        ],
        "configuration": "./languages/mustache.json"
      }
    ],
    "grammars": [
      {
        "language": "mojo-mustache",
        "scopeName": "text.html.mustache.mojo",
        "path": "./grammars/mustache.json"
      }
    ]
  },
  "devDependencies": {},
  "peerDependencies": {},
  "extensionDependencies": [],
  "categories": [
    "Programming Languages",
    "Syntax Highlighting"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  }
}
