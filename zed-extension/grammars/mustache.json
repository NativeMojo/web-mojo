{
  "name": "MOJO Mustache",
  "scopeName": "text.html.mustache.mojo",
  "fileTypes": ["mst"],
  "foldingStartMarker": "(<(?i:(head|table|div|style|script|ul|ol|form|dl))\\b.*?>|{{#)",
  "foldingStopMarker": "(</(?i:(head|table|div|style|script|ul|ol|form|dl))>|{{/)",
  "patterns": [
    {
      "include": "#mustache-comment"
    },
    {
      "include": "#mustache-section"
    },
    {
      "include": "#mustache-inverted-section"
    },
    {
      "include": "#mustache-section-close"
    },
    {
      "include": "#mustache-partial"
    },
    {
      "include": "#mustache-unescaped-triple"
    },
    {
      "include": "#mustache-unescaped-ampersand"
    },
    {
      "include": "#mustache-variable"
    },
    {
      "include": "text.html.basic"
    }
  ],
  "repository": {
    "mustache-comment": {
      "name": "comment.block.mustache.mojo",
      "begin": "{{!",
      "end": "}}",
      "patterns": [
        {
          "match": ".",
          "name": "comment.block.mustache.mojo"
        }
      ]
    },
    "mustache-section": {
      "name": "meta.tag.template.section.mustache.mojo",
      "begin": "{{#",
      "end": "}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-inverted-section": {
      "name": "meta.tag.template.inverted-section.mustache.mojo",
      "begin": "{{\\^",
      "end": "}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-section-close": {
      "name": "meta.tag.template.section-close.mustache.mojo",
      "begin": "{{/",
      "end": "}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-partial": {
      "name": "meta.tag.template.partial.mustache.mojo",
      "begin": "{{>",
      "end": "}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-unescaped-triple": {
      "name": "meta.tag.template.unescaped.mustache.mojo",
      "begin": "{{{",
      "end": "}}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-unescaped-ampersand": {
      "name": "meta.tag.template.unescaped.mustache.mojo",
      "begin": "{{&",
      "end": "}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-variable": {
      "name": "meta.tag.template.variable.mustache.mojo",
      "begin": "{{",
      "end": "}}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.tag.begin.mustache.mojo"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.tag.end.mustache.mojo"
        }
      },
      "patterns": [
        {
          "include": "#mustache-variable-content"
        }
      ]
    },
    "mustache-variable-content": {
      "patterns": [
        {
          "match": "\\s*",
          "name": "text.mustache.mojo"
        },
        {
          "match": "\\.",
          "name": "keyword.operator.accessor.mustache.mojo"
        },
        {
          "match": "\\|iter\\b",
          "name": "keyword.other.iterator.mustache.mojo"
        },
        {
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
          "name": "variable.other.mustache.mojo"
        },
        {
          "match": "[a-zA-Z0-9_.-]+",
          "name": "variable.other.property.mustache.mojo"
        }
      ]
    }
  }
}
